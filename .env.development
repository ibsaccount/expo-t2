# Development Environment Variables
EXPO_PUBLIC_ENV=development
EXPO_PUBLIC_API_URL=https://dev-api.example.com
EXPO_PUBLIC_APP_NAME=Expo T2 (Dev)
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug
EXPO_PUBLIC_ANALYTICS_ENABLED=false

# Database Configuration
EXPO_PUBLIC_DATABASE_URL=https://dev-database.example.com

# Auth Configuration  
EXPO_PUBLIC_AUTH_DOMAIN=dev-auth.example.com
EXPO_PUBLIC_AUTH_CLIENT_ID=dev_client_id_12345

# Feature Flags
EXPO_PUBLIC_ENABLE_BETA_FEATURES=true
EXPO_PUBLIC_ENABLE_DEV_TOOLS=true

# Build Configuration
EXPO_PUBLIC_BUILD_VERSION=1.0.0-dev
EXPO_PUBLIC_BUNDLE_IDENTIFIER=com.ibsaccount.expot2.dev
EXPO_PUBLIC_MIN_SDK_VERSION=35