# Staging Environment Variables
EXPO_PUBLIC_ENV=staging
EXPO_PUBLIC_API_URL=https://staging-api.example.com
EXPO_PUBLIC_APP_NAME=Expo T2 (Staging)
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=warn
EXPO_PUBLIC_ANALYTICS_ENABLED=true

# Database Configuration
EXPO_PUBLIC_DATABASE_URL=https://staging-database.example.com

# Auth Configuration
EXPO_PUBLIC_AUTH_DOMAIN=staging-auth.example.com
EXPO_PUBLIC_AUTH_CLIENT_ID=staging_client_id_12345

# Feature Flags
EXPO_PUBLIC_ENABLE_BETA_FEATURES=true
EXPO_PUBLIC_ENABLE_DEV_TOOLS=false

# Build Configuration
EXPO_PUBLIC_BUILD_VERSION=1.0.0-staging
EXPO_PUBLIC_BUNDLE_IDENTIFIER=com.ibsaccount.expot2.staging
EXPO_PUBLIC_MIN_SDK_VERSION=35