{
  "name": "myapp",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "cp .env.development .env.local && expo start",
    "start:staging": "cp .env.staging .env.local && expo start",
    "start:prod": "cp .env.production .env.local && expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "android:dev": "cp .env.development .env.local && expo start --android",
    "android:staging": "cp .env.staging .env.local && expo start --android",
    "android:prod": "cp .env.production .env.local && expo start --android",
    "ios": "expo start --ios",
    "ios:dev": "cp .env.development .env.local && expo start --ios",
    "ios:staging": "cp .env.staging .env.local && expo start --ios",
    "ios:prod": "cp .env.production .env.local && expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:dev": "eas build --profile development",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:staging": "eas build --profile staging",
    "build:staging:android": "eas build --profile staging --platform android",
    "build:staging:ios": "eas build --profile staging --platform ios",
    "build:staging:aab": "eas build --profile staging-aab --platform android",
    "build:prod": "eas build --profile production",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:aab": "eas build --profile production-aab --platform android",
    "build:preview": "eas build --profile preview",
    "build:preview:aab": "eas build --profile preview-aab --platform android",
    "env:dev": "cp .env.development .env.local",
    "env:staging": "cp .env.staging .env.local",
    "env:prod": "cp .env.production .env.local"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.8.7",
    "@react-navigation/drawer": "^7.7.5",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/material-top-tabs": "^7.4.8",
    "@react-navigation/native": "^7.1.24",
    "@react-navigation/native-stack": "^7.8.1",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.9",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "^6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^4.2.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
